{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "_artifactsLocation": {
            "value": "${artifactsLocationBase}/${project.artifactId}/${git.tag}/src/main/"
        },
        "ibmUserId": {
            "value": "${ibmUserId}"
        },
        "ibmUserPwd": {
            "value": "${ibmUserPwd}"
        },
        "adminUser": {
            "value": "${adminUser}"
        },
        "adminPwd": {
            "value": "${adminPwd}"
        },
        "vmAdminId": {
            "value": "${vmAdminId}"
        },
        "vmAdminPwd": {
            "value": "${vmAdminPwd}"
        },
        "dnsLabelPrefix":{
            "value": "${dnsLabelPrefix}"
        },
        "dmgrVMPrefix": {
            "value": "${dmgrVMPrefix}"
        },
        "managedVMPrefix": {
            "value": "${managedVMPrefix}"
        },
        "numberOfNodes": {
            "value": "${numberOfNodes}"
        },
        "addressPrefix": {
            "value": "${addressPrefix}"
        },
        "subnetName": {
            "value": "${subnetName}"
        },
        "subnetAddressPrefix": {
            "value": "${subnetAddressPrefix}"
        },
        "connectToDB2Server": {
            "value": "${connectToDB2Server}"
        },
        "db2ServerName": {
            "value": "${db2ServerName}"
        },
        "db2ServerPortNumber": {
            "value": "${db2ServerPortNumber}"
        },
        "db2DBName": {
            "value": "${db2DBName}"
        },
        "db2DBUserName": {
            "value": "${db2DBUserName}"
        },
        "db2DBUserPwd": {
            "value": "${db2DBUserPwd}"
        }
    }
}